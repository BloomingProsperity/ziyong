# Ultra Pachong 环境变量配置示例
# 复制此文件为 .env 并填入实际值

# ==================== 代理配置 ====================
# 是否启用代理
PROXY_ENABLED=false

# 代理服务器地址（如使用代理）
PROXY_SERVER=http://proxy.example.com:8080

# 快代理API密钥（如使用快代理）
KUAIDAILI_API_KEY=your_kuaidaili_api_key_here

# ==================== 浏览器配置 ====================
# 是否无头模式
HEADLESS=true

# 浏览器超时时间（毫秒）
BROWSER_TIMEOUT=30000

# ==================== 日志配置 ====================
# 日志级别: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# ==================== 数据库配置 ====================
# PostgreSQL配置（可选）
DB_USER=pachong
DB_PASSWORD=your_secure_password_here
DB_HOST=postgres
DB_PORT=5432
DB_NAME=ultra_pachong

# ==================== 第三方服务 ====================
# 验证码识别服务API密钥（可选）
CAPTCHA_API_KEY=your_captcha_api_key_here

# OpenAI API密钥（可选，用于AI增强功能）
OPENAI_API_KEY=your_openai_api_key_here

# ==================== 其他配置 ====================
# API服务端口
API_PORT=8000

# 最大并发数
MAX_CONCURRENCY=10

# 请求速率限制（每秒）
RATE_LIMIT=5.0
